{
    "name": "innmind/ipc",
    "type": "library",
    "description": "Inter-process communication",
    "keywords": ["process", "communication"],
    "homepage": "http://github.com/Innmind/IPC",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/IPC/issues"
    },
    "require": {
        "php": "~7.2",
        "innmind/immutable": "^2.13",
        "innmind/filesystem": "^3.3",
        "innmind/time-continuum": "^1.3",
        "innmind/operating-system": "^1.4",
        "innmind/socket": "^2.0",
        "innmind/url": "^2.0",
        "innmind/time-warp": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\IPC\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\IPC\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "giorgiosironi/eris": "^0.11.0",
        "innmind/server-status": "^1.3",
        "innmind/server-control": "^2.7"
    }
}
